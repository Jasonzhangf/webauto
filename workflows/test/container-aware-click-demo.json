{
  "name": "容器与Frame感知的点击演示",
  "description": "演示 AdvancedClickNode 在容器与Frame内点击元素的配置用法",
  "version": "1.0.0",
  "nodes": [
    { "id": "start", "type": "StartNode", "name": "开始", "next": ["init_browser"] },
    {
      "id": "init_browser",
      "type": "BrowserInitNode",
      "name": "初始化浏览器",
      "config": { "headless": false, "viewport": { "width": 1440, "height": 900 } },
      "next": ["navigate_demo"]
    },
    {
      "id": "navigate_demo",
      "type": "NavigationNode",
      "name": "打开目标页面",
      "config": { "url": "https://www.baidu.com", "waitUntil": "domcontentloaded" },
      "next": ["click_in_container"]
    },
    {
      "id": "click_in_container",
      "type": "AdvancedClickNode",
      "name": "容器内点击示例",
      "config": {
        "preset": "standard",
        "selector": "#su",
        "containerSelector": "#wrapper",
        "verifyVisibility": true,
        "scrollIntoView": true,
        "highlightElement": true,
        "waitAfter": 1500,
        "verifyNavigation": false
      },
      "next": ["end"]
    },
    {
      "id": "end",
      "type": "EndNode",
      "name": "结束",
      "config": { "cleanup": true }
    }
  ],
  "globalConfig": { "timeout": 120000 }
}

