{
  "name": "1688 简单预登录（仅主页）",
  "description": "只执行预登录到主页，然后停止供手动操作",
  "version": "1.0.0",
  "nodes": [
    { "id": "start", "type": "StartNode", "name": "开始", "next": ["attach_session"] },

    {
      "id": "attach_session",
      "type": "AttachSessionNode",
      "name": "接续预登录会话",
      "config": {},
      "next": ["end"]
    },

    {
      "id": "end",
      "type": "EndNode",
      "name": "停止在主页",
      "config": { "cleanup": false, "saveLogs": true }
    }
  ],
  "globalConfig": {
    "logLevel": "info",
    "screenshotOnError": false,
    "autoCleanup": false,
    "parallelExecution": false,
    "timeout": 60000
  }
}