{
  "name": "webauto",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "camoufox": "^0.1.19",
    "iconv-lite": "^0.6.3",
    "playwright": "^1.56.0",
    "sharp": "^0.33.5"
  },
  "scripts": {
    "start:mcp:browser": "npx @browsermcp/mcp@latest",
    "actions:validate": "node utils/scripts/production/actions-validate.cjs",
