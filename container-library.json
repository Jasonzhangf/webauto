{
  "cbu": {
    "website": "1688.com",
    "containers": {
      "home": {
        "selector": "body, .home, #page",
        "children": [
          "home.search"
        ],
        "description": "1688 首页根容器"
      },
      "home.search": {
        "selector": ".ali-search-box",
        "children": [
          "home.search.searchbox",
          "home.search.searchbutton"
        ],
        "description": "首页搜索区域"
      },
      "home.search.searchbox": {
        "selector": ".ali-search-keywords input#alisearch-input, .ali-search-keywords input[name='keywords'], #alisearch-input, input[name='keywords'], input#alisearch-keywords",
        "actions": {
          "type": true,
          "fill": true
        },
        "description": "首页搜索输入框（位于 .ali-search-box 内）"
      },
      "home.search.searchbutton": {
        "selector": ".ali-search-box .input-button, .ali-search-box .input-button .input-button-text, .input-button .input-button-text",
        "actions": {
          "click": true
        },
        "description": "首页搜索按钮（位于 .ali-search-box 内）"
      },
      "search.root": {
        "selector": ".search-ui2024, .search-i18nUi, body",
        "children": [
          "search.listContainer"
        ],
        "description": "搜索页根容器（新UI类名search-ui2024/i18nUi）"
      },
      "search.listContainer": {
        "selector": ".space-common-offerlist",
        "children": [
          "search.item"
        ],
        "description": "搜索结果列表容器（优先space-common-offerlist）"
      },
      "search.item": {
        "selector": ".search-offer-item",
        "children": [
          "search.item.wangwangLink",
          "search.item.title",
          "search.item.price",
          "search.item.img"
        ],
        "description": "单个结果卡片容器（包含标题、价格、图片、旺旺）"
      },
      "search.item.title": {
        "selector": "h4 a, [class*=title] a, a[title]",
        "description": "标题链接"
      },
      "search.item.price": {
        "selector": "[class*=price], [data-price]",
        "description": "价格区域"
      },
      "search.item.img": {
        "selector": "img",
        "description": "商品图片"
      },
      "search.item.offerUrl": {
        "selector": "a[href*='.1688.com/']",
        "description": "商品/店铺链接"
      },
      "search.item.wangwangSpan": {
        "selector": "span.J_WangWang, span[class*=WangWang]",
        "description": "旺旺容器Span"
      },
      "search.item.wangwangLink": {
        "selector": "span.J_WangWang a.ww-link, a.ww-link[href*='air.1688.com'], a.ww-link[href*='im.1688.com']",
        "actions": {
          "click": true
        },
        "description": "旺旺聊天链接（绑定点击操作）"
      },
      "search.item.shopLink": {
        "selector": "a[href*='company.1688.com'], a[href*='page/index.htm']",
        "description": "公司主页链接"
      },
      "chat.root": {
        "selector": ".ww",
        "children": [
          "chat.inputBox",
          "chat.sendButton"
        ],
        "description": "聊天页根容器（air.1688.com / im.1688.com）"
      },
      "chat.inputBox": {
        "selector": ".editBox [contenteditable=true], .editBox pre.edit, .editBox",
        "actions": {
          "type": true,
          "fill": true
        },
        "description": "聊天输入区域（支持 type/fill）"
      },
      "chat.sendButton": {
        "selector": ".btn-container, .send-btn, button",
        "actions": {
          "click": true
        },
        "description": "聊天发送按钮"
      },
      "1688.home.avatar": {
        "selector": ".userAvatarLogo",
        "description": "头像区（登录成功锚点）"
      },
      "1688.home.header": {
        "selector": ".header",
        "description": "顶部导航区"
      },
      "1688.home.root": {
        "selector": ".home",
        "description": "1688主页-根容器",
        "children": [
          "1688.home.header",
          "1688.home.avatar",
          "1688.home.search"
        ]
      },
      "1688.home.search": {
        "selector": ".ali-search-box",
        "description": "首页搜索区"
      },
      "banner_carousel": {
        "selector": ".banner-carousel",
        "description": "轮播横幅"
      },
      "header_root": {
        "selector": ".header",
        "description": "页头",
        "children": [
          "logo_link",
          "top_shortcuts"
        ]
      },
      "home_root": {
        "selector": ".home",
        "description": "主页根",
        "children": [
          "header_root",
          "search_area",
          "main_nav",
          "banner_carousel",
          "quick_entries",
          "recommend_sections",
          "user_area",
          "site_footer"
        ]
      },
      "hot_suggestions": {
        "selector": ".hot-words",
        "description": "热词推荐"
      },
      "main_nav": {
        "selector": ".site-nav",
        "description": "主导航",
        "children": [
          "nav_category_list"
        ]
      },
      "nav_category_list": {
        "selector": ".nav-category",
        "description": "导航分类列表"
      },
      "quick_entries": {
        "selector": ".quick-entry",
        "description": "快捷入口"
      },
      "recommend_card": {
        "selector": ".offer-item",
        "description": "推荐卡片"
      },
      "recommend_sections": {
        "selector": ".recommend-section",
        "description": "推荐区块",
        "children": [
          "recommend_card"
        ]
      },
      "search_area": {
        "selector": ".search-box",
        "description": "搜索区域",
        "children": [
          "search_input",
          "search_button",
          "hot_suggestions"
        ]
      },
      "search_button": {
        "selector": ".search-btn",
        "description": "搜索按钮"
      },
      "search_input": {
        "selector": ".search-input",
        "description": "搜索输入框"
      },
      "site_footer": {
        "selector": ".site-footer",
        "description": "页脚"
      },
      "user_area": {
        "selector": ".user-info",
        "description": "用户区域",
        "children": [
          "user_avatar",
          "user_name",
          "ww_link"
        ]
      },
      "user_avatar": {
        "selector": ".userAvatarLogo",
        "description": "用户头像"
      },
      "user_name": {
        "selector": ".member-nickname",
        "description": "用户昵称"
      },
      "ww_link": {
        "selector": ".ww-link",
        "description": "旺旺链接"
      }
    }
  }
}