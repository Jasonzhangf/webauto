{
  "name": "1688 自然搜索工作流（已验证版）",
  "description": "基于成功的主页访问，实现自然搜索：主页→搜索框输入→点击搜索→获取结果",
  "version": "1.0.0",
  "nodes": [
    { "id": "start", "type": "StartNode", "name": "开始", "next": ["attach"] },
    { "id": "attach", "type": "AttachSessionNode", "name": "会话接力", "config": {}, "next": ["navigate_home"] },

    {
      "id": "navigate_home",
      "type": "NavigationNode",
      "name": "导航到主页",
      "config": {
        "url": "https://www.1688.com/",
        "waitUntil": "domcontentloaded",
        "timeout": 30000
      },
      "next": ["antibot_soft"]
    },

    {
      "id": "antibot_soft",
      "type": "AntiBotMitigationNode",
      "name": "轻量反风控",
      "config": { "mouseMoves": 3, "scroll": true, "jitterMinMs": 1000, "jitterMaxMs": 2500 },
      "next": ["dismiss_modals"]
    },

    {
      "id": "dismiss_modals",
      "type": "ModalDismissNode",
      "name": "关闭模态",
      "config": { "maxAttempts": 5, "retryDelay": 500 },
      "next": ["wait_home"]
    },

    {
      "id": "wait_home",
      "type": "WaitNode",
      "name": "等待主页稳定",
      "config": { "minMs": 3000, "maxMs": 5000 },
      "next": ["check_search_box"]
    },

    {
      "id": "check_search_box",
      "type": "PageSnapshotNode",
      "name": "检查搜索框状态",
      "config": {
        "script": "console.log('🔍 检查1688主页搜索框...'); const searchBox = document.querySelector('#q'); if (searchBox && searchBox.offsetWidth > 0 && searchBox.offsetHeight > 0) { console.log('✅ 找到搜索框:', searchBox.placeholder); const searchButton = document.querySelector('.search-btn'); console.log('搜索按钮:', !!searchButton); return { success: true, searchBox: { found: true, placeholder: searchBox.placeholder, visible: true }, searchButton: { found: !!searchButton, visible: searchButton ? (searchButton.offsetWidth > 0 && searchButton.offsetHeight > 0) : false }, pageTitle: document.title, pageUrl: window.location.href }; } else { console.log('❌ 搜索框不可用'); return { success: false, searchBox: { found: false }, pageTitle: document.title, pageUrl: window.location.href }; }",
        "saveScreenshots": true
      },
      "next": ["input_search"]
    },

    {
      "id": "input_search",
      "type": "PageSnapshotNode",
      "name": "输入搜索关键词",
      "config": {
        "script": "console.log('📝 开始输入搜索关键词...'); const searchBox = document.querySelector('#q'); if (searchBox) { searchBox.focus(); searchBox.value = ''; searchBox.value = 'phone'; console.log('✅ 已输入关键词: phone'); const inputEvent = new Event('input', { bubbles: true }); searchBox.dispatchEvent(inputEvent); return { success: true, keyword: 'phone', searchBoxValue: searchBox.value }; } else { console.log('❌ 未找到搜索框'); return { success: false, error: 'Search box not found' }; }",
        "saveScreenshots": true
      },
      "next": ["wait_after_input"]
    },

    {
      "id": "wait_after_input",
      "type": "WaitNode",
      "name": "输入后等待",
      "config": { "minMs": 1000, "maxMs": 2000 },
      "next": ["click_search"]
    },

    {
      "id": "click_search",
      "type": "PageSnapshotNode",
      "name": "点击搜索按钮",
      "config": {
        "script": "console.log('🔍 点击搜索按钮...'); const searchButton = document.querySelector('.search-btn'); if (searchButton && searchButton.offsetWidth > 0 && searchButton.offsetHeight > 0) { console.log('✅ 找到搜索按钮，准备点击'); searchButton.click(); return { success: true, action: 'clicked_search_button', buttonText: searchButton.textContent.trim() }; } else { console.log('❌ 搜索按钮不可用'); const alternativeButton = document.querySelector('button[type=submit], .search-btn button, [class*=\"search\"] button'); if (alternativeButton) { console.log('🔄 尝试替代搜索按钮'); alternativeButton.click(); return { success: true, action: 'clicked_alternative_button', buttonText: alternativeButton.textContent.trim() }; } return { success: false, error: 'Search button not found or not clickable' }; }",
        "saveScreenshots": true
      },
      "next": ["wait_search_results"]
    },

    {
      "id": "wait_search_results",
      "type": "WaitNode",
      "name": "等待搜索结果加载",
      "config": { "minMs": 5000, "maxMs": 7000 },
      "next": ["check_search_results"]
    },

    {
      "id": "check_search_results",
      "type": "PageSnapshotNode",
      "name": "检查搜索结果页面",
      "config": {
        "script": "console.log('🔍 检查搜索结果页面...'); const currentUrl = window.location.href; const pageTitle = document.title; console.log('当前页面:', currentUrl, pageTitle); const isSearchPage = currentUrl.includes('s.1688.com') || currentUrl.includes('search'); const hasResults = document.querySelectorAll('.sm-offer-item, .offer-item, [class*=\"offer\"]').length > 0; const isVerification = pageTitle.includes('验证码') || document.body.textContent.includes('验证码') || document.body.textContent.includes('verification'); console.log('是否为搜索页面:', isSearchPage); console.log('是否有搜索结果:', hasResults); console.log('是否被验证码拦截:', isVerification); return { success: !isVerification && (isSearchPage || hasResults), isSearchPage, hasResults, isVerification, pageTitle, pageUrl: currentUrl, resultCount: document.querySelectorAll('.sm-offer-item, .offer-item, [class*=\"offer\"]').length };",
        "saveScreenshots": true
      },
      "next": ["save_results"]
    },

    {
      "id": "save_results",
      "type": "ResultSaverNode",
      "name": "保存搜索结果",
      "config": {
        "outputDir": "workflows/records",
        "filenameTemplate": "1688-natural-search-working-{timestamp}.json",
        "includeMetadata": true
      },
      "next": ["end"]
    },

    {
      "id": "end",
      "type": "EndNode",
      "name": "结束",
      "config": { "cleanup": false, "saveLogs": true }
    }
  ],
  "globalConfig": {
    "logLevel": "info",
    "screenshotOnError": true,
    "autoCleanup": false,
    "parallelExecution": false,
    "timeout": 1800000
  }
}