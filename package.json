{
  "name": "webauto",
  "private": true,
  "version": "0.1.003",
  "type": "module",
  "dependencies": {
    "esbuild-register": "^3.6.0",
    "iconv-lite": "^0.6.3",
    "linkedom": "^0.18.12",
    "minimist": "^1.2.8",
    "node-fetch": "^2.7.0",
    "sharp": "^0.33.5",
    "undici": "^7.16.0",
    "ws": "^8.18.3"
  },
  "scripts": {
    "start:mcp:browser": "npx @browsermcp/mcp@latest",
    "actions:validate": "node scripts/actions-validate.cjs",
    "actions:run": "node scripts/run-action.cjs",
    "build": "npm run prebuild && npm run build:services && npm run test:floating-panel:logic && npm run self-check:post-build && npm run ui:test",
    "build:services": "node scripts/build/run-services-build.mjs",
    "build:ocr:macos": "node scripts/build/build-ocr-macos.mjs",
    "build:package": "node scripts/package/build-cli.mjs",
    "package": "npm run build:services && npm run build:package",
    "install:macos": "node scripts/install-macos.mjs",
    "self-check:post-build": "node scripts/self-check.mjs --quick --post-build",
    "start": "node dist/src/server.js",
    "start:workflow-api": "node dist/sharedmodule/engines/api-gateway/server.js",
    "start:vision-proxy": "node dist/sharedmodule/engines/vision-engine/server.js",
    "start:orchestrator": "node dist/src/server.js",
    "start:vision": "node dist/sharedmodule/engines/vision-engine/server.js",
    "test:services": "node runtime/infra/utils/scripts/service-tests/run-all.mjs",
    "test:vision": "node scripts/run-vision-tests.mjs",
    "check:modules": "echo \"Module checks skipped\"",
    "check:ts": "tsc --noEmit -p tsconfig.services.json",
    "test:modules:unit": "npx tsx --test $(find modules -name \"*.test.ts\" -o -name \"*.test.mts\" 2>/dev/null | tr \"\\n\" \" \")",
    "test": "npm run check:modules && npm run check:ts && npm run test:modules:unit",
    "cli:browser-control": "tsx modules/browser-control/src/cli.ts",
    "cli:session-manager": "tsx modules/session-manager/src/cli.ts",
    "cli:logging": "tsx modules/logging/src/cli.ts",
    "cli:operations": "tsx modules/operations/src/cli.ts",
    "cli:container-registry": "tsx modules/container-registry/src/cli.ts",
    "cli:container-matcher": "tsx modules/container-matcher/src/cli.ts",
    "cli:dom-branch": "node modules/dom-branch-fetcher/src/cli.ts",
    "ui:test": "echo 'UI tests migrated to runtime/infra/utils/scripts/development/'",
    "ui:test:driver": "node runtime/infra/utils/scripts/development/eval-in-session.mjs",
    "ui:state": "node runtime/infra/utils/scripts/development/highlight-search-containers.mjs",
    "browser:oneclick": "node runtime/browser/scripts/one-click-browser.mjs",
    "browser:oneclick:no-check": "node runtime/browser/scripts/one-click-browser.mjs --skip-health",
    "browser:oneclick:health-only": "node runtime/browser/scripts/one-click-browser.mjs --health-only",
    "browser:oneclick:strict": "WEBAUTO_STRICT_CHECK=1 node runtime/browser/scripts/one-click-browser.mjs",
    "service:browser:start": "node runtime/infra/utils/scripts/service/start-browser-service.mjs",
    "service:browser:stop": "node runtime/infra/utils/scripts/service/stop-browser-service.mjs",
    "service:browser:restart": "node runtime/infra/utils/scripts/service/restart-browser-service.mjs",
    "service:browser:restart:python": "node runtime/infra/utils/scripts/service/restart-python-browser-service.mjs",
    "service:controller:start": "node services/controller/src/server.mjs",
    "service:kill-port": "node runtime/infra/utils/scripts/service/kill-port.mjs",
    "browser:camoufox:oneclick": "node runtime/browser/scripts/one-click-camoufox.mjs",
    "browser:camoufox:status": "node runtime/browser/scripts/status-camoufox.mjs",
    "browser:camoufox:navigate1688": "node runtime/browser/scripts/navigate-1688.mjs",
    "browser:camoufox:install": "python3 -m camoufox remove && python3 -m camoufox fetch",
    "browser:highlight": "node scripts/ui/send-highlight-cli.mjs",
    "browser:clear-highlight": "node scripts/ui/send-highlight-cli.mjs --clear",
    "test:graph-engine": "tsx --test modules/graph-engine/tests/**/*.ts",
    "test:dom-branch": "tsx --test modules/dom-branch-fetcher/tests/**/*.ts",
    "test:floating-orchestrator": "tsx apps/floating-panel/core/__tests__/orchestrator.test.mjs",
    "test:floating-panel:logic": "cd apps/floating-panel && npm run test:logic",
    "prebuild": "node scripts/self-check.mjs --quick",
    "self-check": "node scripts/self-check.mjs",
    "self-check:quick": "node scripts/self-check.mjs --quick",
    "self-check:fix": "node scripts/self-check.mjs --fix",
    "health-check": "node scripts/health-check.mjs",
    "health-check:quick": "node scripts/health-check.mjs --quick",
    "health-check:stage": "node scripts/health-check.mjs --stage",
    "health-check:runtime": "node scripts/health-check-runtime.mjs",
    "health-check:runtime:watch": "node scripts/health-check-runtime.mjs --watch",
    "launch:diagnose": "node scripts/launch-with-diagnostics.mjs",
    "launch:diagnose:ui": "node scripts/launch-with-diagnostics.mjs --headless=false",
    "launch:diagnose:dev": "node scripts/launch-with-diagnostics.mjs --dev"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@types/express": "^5.0.6",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.10.1",
    "@types/ws": "^8.18.1",
    "camoufox": "^0.1.19",
    "json-schema-to-typescript": "^15.0.4",
    "playwright": "^1.57.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.5",
    "typescript": "^5.9.3"
  }
}
