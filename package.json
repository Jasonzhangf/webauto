{
  "name": "webauto",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "linkedom": "^0.18.12",
    "robotjs": "^0.6.0",
    "sharp": "^0.33.5",
    "ws": "^8.18.3"
  },
  "scripts": {
    "start:mcp:browser": "npx @browsermcp/mcp@latest",
    "actions:validate": "node scripts/actions-validate.cjs",
    "actions:run": "node scripts/run-action.cjs",
    "build": "npm run build:services && npm run ui:test",
    "build:services": "node scripts/build/run-services-build.mjs",
    "start": "node dist/src/server.js",
    "start:workflow-api": "node dist/sharedmodule/engines/api-gateway/server.js",
    "start:vision-proxy": "node dist/sharedmodule/engines/vision-engine/server.js",
    "start:orchestrator": "node dist/src/server.js",
    "start:vision": "node dist/sharedmodule/engines/vision-engine/server.js",
    "test:services": "node scripts/service-tests/run-all.mjs",
    "test:vision": "node scripts/run-vision-tests.mjs",
    "check:modules": "node scripts/test-modules.mjs",
    "check:ts": "tsc --noEmit -p tsconfig.services.json",
    "test:modules:unit": "tsx --test modules/**/*.test.ts",
    "test": "npm run check:modules && npm run check:ts && npm run test:modules:unit",
    "cli:browser-control": "tsx modules/browser-control/src/cli.ts",
    "cli:session-manager": "tsx modules/session-manager/src/cli.ts",
    "cli:logging": "tsx modules/logging/src/cli.ts",
    "cli:operations": "tsx modules/operations/src/cli.ts",
    "cli:container-registry": "tsx modules/container-registry/src/cli.ts",
    "cli:container-matcher": "tsx modules/container-matcher/src/cli.ts",
    "cli:dom-branch": "node modules/dom-branch-fetcher/src/cli.ts",
    "ui:test": "node scripts/ui/run-headless-tests.mjs",
    "ui:test:driver": "node scripts/ui/dev-driver.mjs",
    "ui:state": "node scripts/ui/get-ui-state.mjs",
    "browser:oneclick": "node runtime/browser/scripts/one-click-browser.mjs",
    "verify": "node scripts/check/self-verify.mjs",
    "service:browser:start": "node runtime/infra/utils/scripts/service/start-browser-service.mjs",
    "service:browser:stop": "node runtime/infra/utils/scripts/service/stop-browser-service.mjs",
    "service:browser:restart": "node runtime/infra/utils/scripts/service/restart-browser-service.mjs",
    "service:browser:restart:python": "node runtime/infra/utils/scripts/service/restart-python-browser-service.mjs",
    "service:controller:start": "node services/controller/src/server.mjs",
    "service:kill-port": "node runtime/infra/utils/scripts/service/kill-port.mjs",
    "browser:camoufox:oneclick": "node runtime/browser/scripts/one-click-camoufox.mjs",
    "browser:camoufox:status": "node runtime/browser/scripts/status-camoufox.mjs",
    "browser:camoufox:navigate1688": "node runtime/browser/scripts/navigate-1688.mjs",
    "browser:camoufox:install": "python3 -m camoufox remove && python3 -m camoufox fetch",
    "browser:highlight": "node scripts/ui/send-highlight-cli.mjs",
    "browser:clear-highlight": "node scripts/ui/send-highlight-cli.mjs --clear",
    "test:graph-engine": "tsx --test modules/graph-engine/tests/**/*.ts",
    "test:dom-branch": "tsx --test modules/dom-branch-fetcher/tests/**/*.ts",
    "test:floating-orchestrator": "tsx apps/floating-panel/core/__tests__/orchestrator.test.mjs"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@types/node": "^24.10.1",
    "@types/ws": "^8.18.1",
    "camoufox": "^0.1.19",
    "playwright": "^1.57.0",
    "tsx": "^4.16.5",
    "typescript": "^5.6.3"
  }
}
