  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@types/node": "^24.10.1",
    "@types/ws": "^8.18.1",
    "camoufox": "^0.1.19",
    "playwright": "^1.57.0"
  }
}
