{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://webauto/containers/index.v2.schema.json",
  "title": "ContainerIndexV2",
  "type": "object",
  "required": ["website", "generatedAt", "containers"],
  "properties": {
    "website": { "type": "string" },
    "generatedAt": { "type": "string" },
    "containerCount": { "type": "number" },
    "containers": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "fileName", "selector"],
        "properties": {
          "id": { "type": "string" },
          "fileName": { "type": "string" },
          "selector": { "type": "string" }
        },
        "additionalProperties": true
      }
    },
    "searchIndex": { "type": "object" }
  },
  "additionalProperties": true
}

