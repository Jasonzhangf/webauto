{
  "name": "@webauto/operator-framework",
  "version": "0.0.1",
  "description": "WebAuto Operator Framework - 基于嵌套operator的自动化操作框架",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "tsc --project tsconfig.test.json && node dist-tests/tests/container-system-test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist dist-tests"
  },
  "keywords": [
    "automation",
    "operator",
    "browser",
    "web-scraping",
    "rcc-basemodule",
    "nested-operators"
  ],
  "author": "WebAuto Team",
  "license": "MIT",
  "dependencies": {
    "rcc-basemodule": "^0.2.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
