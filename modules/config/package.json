{
  "name": "@webauto/config",
  "version": "1.0.0",
  "description": "Unified configuration management module for WebAuto",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "tsx --test src/*.test.ts",
    "test:coverage": "c8 --reporter=text --reporter=lcov tsx --test src/*.test.ts",
    "test:watch": "tsx --test --watch src/*.test.ts"
  },
  "keywords": [
    "config",
    "configuration",
    "json-schema"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "c8": "^10.1.0",
    "tsx": "^4.16.5",
    "typescript": "^5.3.0"
  }
}
