{
  "name": "@webauto/cli",
  "version": "1.0.0",
  "description": "WebAuto Browser CLI - Node.js wrapper for Python CLI",
  "main": "dist/index.js",
  "bin": {
    "webauto": "./bin/webauto.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist"
  },
  "keywords": ["browser", "automation", "cli", "web", "scraping"],
  "author": "WebAuto Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.11",
    "chalk": "^5.3.0",
    "ws": "^8.14.2",
    "js-yaml": "^4.1.0",
    "config": "^3.3.10",
    "cross-spawn": "^7.0.3",
    "lodash": "^4.17.21",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/ws": "^8.5.8",
    "@types/js-yaml": "^4.0.8",
    "@types/cross-spawn": "^6.0.4",
    "@types/lodash": "^4.14.200",
    "@types/jest": "^29.5.6",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webauto/cli"
  },
  "bugs": {
    "url": "https://github.com/webauto/cli/issues"
  },
  "homepage": "https://github.com/webauto/cli#readme"
}