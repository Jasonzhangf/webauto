{
  "id": "weibo_login.username_input",
  "name": "用户名输入框",
  "type": "input",
  "capabilities": ["input", "highlight"],
  "selectors": [
    { "css": "#app input[name='username']", "variant": "primary", "score": 1.0 },
    { "css": "#app input[type='text'][name]", "variant": "backup", "score": 0.8 },
    { "css": "#app input[type='text'][placeholder*='手机号'], #app input[type='text'][placeholder*='邮箱']", "variant": "backup", "score": 0.6 }
  ],
  "operations": [
    {
      "type": "highlight",
      "config": {
        "style": "2px solid #4285f4",
        "duration": 2000
      }
    },
    {
      "type": "type",
      "config": {
        "clear_first": true,
        "human_typing": true
      }
    }
  ]
}
