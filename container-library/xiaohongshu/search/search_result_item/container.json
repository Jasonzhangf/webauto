{
  "id": "xiaohongshu_search.search_result_item",
  "name": "搜索结果项",
  "type": "item",
  "capabilities": [
    "highlight",
    "extract",
    "click",
    "navigate"
  ],
  "children": [],
  "selectors": [
    {
      "css": ".note-item:has(a.cover)",
      "variant": "primary",
      "score": 1.0
    }
  ],
  "metadata": {
    "fields": [
      "title",
      "link",
      "detail_url",
      "image",
      "text",
      "note_id"
    ],
    "required_descendants_any": [
      "a.cover"
    ],
    "excluded_descendants_any": [
      ".query-note-wrapper"
    ]
  },
  "extractors": {
    "title": {
      "selectors": [
        ".footer .title span",
        ".footer .title"
      ],
      "attr": "textContent"
    },
    "link": {
      "selectors": [
        "a.cover"
      ],
      "attr": "href"
    },
    "detail_url": {
      "selectors": [
        "a.cover"
      ],
      "attr": "href"
    },
    "image": {
      "selectors": [
        "a.cover img"
      ],
      "attr": "src"
    },
    "note_id": {
      "selectors": [
        "a.cover"
      ],
      "attr": "href"
    }
  },
  "operations": [
    {
      "type": "highlight",
      "config": {
        "style": "2px solid #ea4335",
        "duration": 1500
      }
    },
    {
      "type": "click",
      "config": {
        "target": "a.cover",
        "comment": "点击笔记封面区域（a.cover）"
      }
    },
    {
      "type": "extract",
      "config": {
        "fields": ["title", "detail_url", "note_id"]
      }
    }
  ]
}
