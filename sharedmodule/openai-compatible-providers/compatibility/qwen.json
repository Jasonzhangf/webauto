{
  "provider": "qwen",
  "version": "1.0.0",
  "description": "Qwen AI Provider with OAuth 2.0 Device Flow support",
  "authentication": {
    "type": "oauth2",
    "flow": "device_code",
    "configuration": {
      "client_id": "f0304373b74a44d2b584a3fb70ca9e56",
      "device_code_url": "https://chat.qwen.ai/api/v1/oauth2/device/code",
      "token_url": "https://chat.qwen.ai/api/v1/oauth2/token",
      "scopes": [
        "openid",
        "profile", 
        "email",
        "model.completion"
      ],
      "pkce": true,
      "token_refresh": true
    }
  },
  "api": {
    "base_url": "https://portal.qwen.ai/v1",
    "endpoints": {
      "chat_completions": "/chat/completions",
      "models": "/models"
    },
    "timeout": 30000,
    "retry_attempts": 3,
    "retry_delay": 1000
  },
  "models": [
    {
      "id": "qwen3-coder-plus",
      "name": "Qwen 3 Coder Plus",
      "description": "Advanced coding model with enhanced reasoning capabilities",
      "context_length": 32768,
      "max_output_tokens": 8192,
      "capabilities": {
        "streaming": true,
        "function_calling": true,
        "vision": false,
        "json_mode": true,
        "code_generation": true
      },
      "parameters": {
        "temperature": {
          "type": "number",
          "minimum": 0,
          "maximum": 2,
          "default": 1,
          "description": "Controls randomness in generation"
        },
        "top_p": {
          "type": "number", 
          "minimum": 0,
          "maximum": 1,
          "default": 0.95,
          "description": "Controls diversity via nucleus sampling"
        },
        "top_k": {
          "type": "number",
          "minimum": 1,
          "maximum": 100,
          "default": 64,
          "description": "Controls diversity via top-k sampling"
        },
        "max_tokens": {
          "type": "integer",
          "minimum": 1,
          "maximum": 8192,
          "default": 4096,
          "description": "Maximum number of tokens to generate"
        }
      },
      "pricing": {
        "input": 0.001,
        "output": 0.002,
        "currency": "USD"
      },
      "tags": ["coding", "reasoning", "advanced"]
    },
    {
      "id": "qwen3-coder-flash",
      "name": "Qwen 3 Coder Flash",
      "description": "Fast coding model optimized for quick iterations and development",
      "context_length": 16384,
      "max_output_tokens": 4096,
      "capabilities": {
        "streaming": true,
        "function_calling": false,
        "vision": false,
        "json_mode": true,
        "code_generation": true
      },
      "parameters": {
        "temperature": {
          "type": "number",
          "minimum": 0,
          "maximum": 2,
          "default": 1,
          "description": "Controls randomness in generation"
        },
        "top_p": {
          "type": "number",
          "minimum": 0, 
          "maximum": 1,
          "default": 0.95,
          "description": "Controls diversity via nucleus sampling"
        },
        "top_k": {
          "type": "number",
          "minimum": 1,
          "maximum": 100,
          "default": 64,
          "description": "Controls diversity via top-k sampling"
        },
        "max_tokens": {
          "type": "integer",
          "minimum": 1,
          "maximum": 4096,
          "default": 2048,
          "description": "Maximum number of tokens to generate"
        }
      },
      "pricing": {
        "input": 0.0005,
        "output": 0.001,
        "currency": "USD"
      },
      "tags": ["coding", "fast", "development"]
    }
  ],
  "format_mapping": {
    "request": {
      "standard_format": "openai",
      "provider_format": "qwen",
      "transformations": {
        "messages": "direct_mapping",
        "temperature": "direct_mapping",
        "top_p": "direct_mapping",
        "max_tokens": "direct_mapping",
        "tools": "conditional_mapping"
      }
    },
    "response": {
      "standard_format": "openai",
      "provider_format": "qwen", 
      "transformations": {
        "id": "generate_if_missing",
        "object": "map_to_standard",
        "created": "generate_timestamp",
        "choices": "standardize_structure",
        "usage": "direct_mapping"
      }
    }
  },
  "error_handling": {
    "retry_on": [
      "network_error",
      "timeout_error",
      "rate_limit_error"
    ],
    "max_retries": 3,
    "retry_delay": 1000,
    "exponential_backoff": true
  },
  "features": {
    "oauth_device_flow": true,
    "token_refresh": true,
    "streaming": true,
    "function_calling": true,
    "model_discovery": true,
    "health_check": true
  },
  "configuration": {
    "default_model": "qwen3-coder-plus",
    "fallback_model": "qwen3-coder-flash",
    "auto_refresh_token": true,
    "token_expiry_buffer": 300,
    "request_timeout": 30000
  },
  "metadata": {
    "provider_name": "Qwen",
    "provider_website": "https://qwen.aliyun.com",
    "documentation": "https://qwen.readthedocs.io",
    "support_email": "qwen-support@alibaba-cloud.com",
    "last_updated": "2024-01-12",
    "compatibility_version": "1.0.0"
  }
}