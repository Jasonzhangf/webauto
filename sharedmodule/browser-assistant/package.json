{
  "name": "@webauto/browser-assistant",
  "version": "0.1.0",
  "description": "Intelligent browser automation assistant with Camoufox and AI-powered page analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:simple": "tsc -p tsconfig-simple.json",
    "build:standalone": "tsc && node scripts/post-build.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist dist-simple",
    "prepack": "npm run clean && npm run build",
    "example": "ts-node examples/basic-usage.ts",
    "example:advanced": "ts-node examples/advanced-usage.ts",
    "example:analysis": "ts-node examples/page-analysis-example.ts",
    "prepare": "echo 'Skipping build during install'"
  },
  "keywords": [
    "browser",
    "automation",
    "camoufox",
    "playwright",
    "ai",
    "page-analysis",
    "web-scraping",
    "assistant"
  ],
  "author": "WebAuto Team",
  "license": "MIT",
  "dependencies": {
    "rcc-basemodule": "^0.1.4",
    "rcc-errorhandling": "^1.0.5",
    "rcc-underconstruction": "^0.1.0",
    "camoufox": "^0.1.12",
    "playwright": "^1.40.0",
    "zod": "^3.22.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}