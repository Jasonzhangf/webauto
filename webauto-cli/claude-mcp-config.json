{
  "name": "webauto-cli",
  "description": "An automated web processing program with MCP architecture",
  "version": "0.0.1",
  "api_version": "2024-01-01",
  "transport": "stdio",
  "handlers": {
    "initialize": "src/mcp/stdio-transport.js",
    "tools/list": "src/mcp/stdio-transport.js",
    "tools/call": "src/mcp/stdio-transport.js",
    "resources/list": "src/mcp/stdio-transport.js",
    "resources/read": "src/mcp/stdio-transport.js",
    "prompts/list": "src/mcp/stdio-transport.js",
    "prompts/get": "src/mcp/stdio-transport.js"
  },
  "env": {
    "DEBUG": "true"
  },
  "instructions": [
    "This server allows you to manage web automation pipelines and rules",
    "You can execute pipelines, apply rules to webpages, and extract targets",
    "Use the available tools to perform these operations"
  ],
  "capabilities": {
    "resources": {
      "list": true,
      "read": true
    },
    "prompts": {
      "list": true,
      "get": true
    },
    "tools": {
      "list": true,
      "call": true
    }
  }
}