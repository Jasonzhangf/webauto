{
  "name": "webauto-cli",
  "version": "0.0.1",
  "description": "An automated web processing program with MCP architecture",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "mcp": "node src/mcp/stdio-transport.js",
    "test": "jest",
    "build": "npm run lint && npm run test",
    "lint": "echo 'Linting...' && exit 0",
    "preinstall": "echo 'Pre-install checks...' && node -c src/mcp/stdio-transport.js",
    "postinstall": "echo 'Installation complete'",
    "install:global": "npm run build && npm uninstall -g webauto-cli && npm install -g ."
  },
  "keywords": [
    "mcp",
    "web automation",
    "camoufox",
    "pipeline"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "rcc-basemodule": "^0.1.4",
    "rcc-errorhandling": "^1.0.5",
    "rcc-underconstruction": "^0.1.0",
    "camoufox": "^0.1.12",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "nodemon": "^3.0.0"
  },
  "mcp": {
    "config": "./iflow-mcp-config.json"
  },
  "bin": {
    "webauto": "./src/cli.js",
    "webauto-mcp": "./src/mcp/stdio-transport.js"
  }
}